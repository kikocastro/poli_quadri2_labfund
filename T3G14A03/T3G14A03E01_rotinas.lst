; ###################################
; MEMCPY
; ###################################
;
; Variaveis
;
805c 0000		;	COUNT                   K       /0000
805e 0000		;	MEMCPY_ORIGIN_PTR       K       /0000
8060 0000		;	MEMCPY_DESTINATION_PTR  K       /0000
8062 0000		;	MEMCPY_SIZE_PTR         K       /0000
8064 0000		;	MEMCPY_ORIGIN           K       /0000
8066 0000		;	MEMCPY_DESTINATION      K       /0000
8068 0000		;	MEMCPY_SIZE             K       /0000
;
; Rotina
;
806a 0000		;	MEMCPY                  K       /0000
                        ; Carrega valores de entrada
a06c 805e		;	                        LD      MEMCPY_ORIGIN_PTR ; carrega o endereço
d06e 4000		;	                        +       LOAD ; Soma load
a070 9072		;	                        MM      UNPACK_EXEC1 ; Armazena em UNPACK_EXEC1
8072 0000		;	UNPACK_EXEC1            K       /0000 ; Carrega valor do endereço
a074 9064		;	                        MM      MEMCPY_ORIGIN ; Armazena
a076 8060		;	                        LD      MEMCPY_DESTINATION_PTR ; carrega o endereço
d078 4000		;	                        +       LOAD ; Soma load
a07a 907c		;	                        MM      UNPACK_EXEC2 ; Armazena em UNPACK_EXEC2
807c 0000		;	UNPACK_EXEC2            K       /0000 ; Carrega valor do endereço
a07e 9066		;	                        MM      MEMCPY_DESTINATION ; Armazena
a080 8062		;	                        LD      MEMCPY_SIZE_PTR ; carrega o endereço
d082 4000		;	                        +       LOAD ; Soma load
a084 9086		;	                        MM      UNPACK_EXEC3 ; Armazena em UNPACK_EXEC3
8086 0000		;	UNPACK_EXEC3            K       /0000 ; Carrega valor do endereço
a088 9068		;	                        MM      MEMCPY_SIZE ; Armazena
                        ; Tratamento de erros de Input
                        ; 1) Endereço inicial + o numero de palavras > endereço maximo
d08a 8011		;	                        LD      RANGE_END   ; Carrega o endereço maximo de destino
a08c 5068		;	                        -       MEMCPY_SIZE
a08e 5068		;	                        -       MEMCPY_SIZE ; Subtrai numero de enderecos de words que serao copiadas (Duas vezes pois cada palavra ocupa 2 Bytes)
a090 5066		;	                        -       MEMCPY_DESTINATION  ; Subtrai o endereço inicial do destino
a092 20d6		;	                        JN      END_FAIL  ; Caso o endereço inicial + o numero de palavras > endereço maximo, ERRO
                        ; 2) Endereço de destino esta antes do intervalo
a094 8066		;	                        LD      MEMCPY_DESTINATION
d096 5010		;	                        -       RANGE_START   
a098 20d6		;	                        JN      END_FAIL  
                        ; 3) Origem esta antes do intervalo
a09a 8064		;	                        LD      MEMCPY_ORIGIN
d09c 5010		;	                        -       RANGE_START
a09e 20d6		;	                        JN      END_FAIL   
                        ; 4) Origem esta a frente do intervalo
d0a0 8011		;	                        LD      RANGE_END
a0a2 5064		;	                        -       MEMCPY_ORIGIN
a0a4 5068		;	                        -       MEMCPY_SIZE
a0a6 5068		;	                        -       MEMCPY_SIZE
a0a8 20d6		;	                        JN      END_FAIL  
                        ; Comeco de MEMCPY
a0aa 8068		;	LOOP                    LD      MEMCPY_SIZE ; Carrega o numero de words no acumulador
a0ac 505c		;	                        -       COUNT     ; Subtrai o contador do acumulador
a0ae 10d2		;	                        JZ      END_SUCCESS ; Caso o contador seja igual ao numero de words, encerra
                        
a0b0 8066		;	                        LD      MEMCPY_DESTINATION ; Carrega endereco de destino
d0b2 4001		;	                        +       WRITE   ; Adiciona comando MM
a0b4 90bc		;	                        MM      MEMCPY_EXEC   ; Armazena em MEMCPY_EXEC
                        
a0b6 8064		;	                        LD      MEMCPY_ORIGIN ; Carrega endereço de origem
a0b8 90da		;	                        MM      TARGET_ADDRESS 
a0ba a0dc		;	                        SC      LOAD_VALUE ; Carrega valor no endereco de origem

80bc 0000		;	MEMCPY_EXEC             K       /0000 ; Armazena o valor no endereco de destino
a0be 8066		;	                        LD      MEMCPY_DESTINATION  ; Carrega o endereço de destino
d0c0 4004		;	                        +       CONST_2   ; Avança 2 posições na memoria
a0c2 9066		;	                        MM      MEMCPY_DESTINATION  ; Atualiza MEMCPY_DESTINATION
                        
a0c4 8064		;	                        LD      MEMCPY_ORIGIN ; Carrega o endereço de origem
d0c6 4004		;	                        +       CONST_2   ; Avança 2 posições na memoria
a0c8 9064		;	                        MM      MEMCPY_ORIGIN ; Atualiza MEMCPY_ORIGIN
                        
a0ca 805c		;	                        LD      COUNT     ; Carrega o contador no acumulador
d0cc 4003		;	                        +       CONST_1     ; Soma 1
a0ce 905c		;	                        MM      COUNT     ; Atualiza CONT
                        
a0d0 00aa		;	                        JP      LOOP

d0d2 8002		;	END_SUCCESS             LD      CONST_0   ; Se o programa finalizar com sucesso, coloca 0x0000 no acumulador
a0d4 00d8		;	                        JP      RETURN_MEMCPY 
d0d6 8009		;	END_FAIL                LD      CONST_FFFF ; Se o programa finalizar com falhas, coloca 0xFFFF no acumulador
          
a0d8 b06a		;	RETURN_MEMCPY           RS      MEMCPY
